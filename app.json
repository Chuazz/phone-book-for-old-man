{
	"expo": {
		"name": "Danh bạ nè",
		"slug": "phone-book-for-old-man",
		"version": "0.0.0",
		"orientation": "portrait",
		"icon": "./assets/icons/ic_app_logo.png",
		"scheme": "phone-book-for-old-man",
		"assetBundlePatterns": ["**/*"],
		"userInterfaceStyle": "automatic",
